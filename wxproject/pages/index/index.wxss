/**index.wxss**/
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}

.usermotto {
  margin-top: 200px;
}

.go-list-wrapper{
  display: flex;
  margin-top: 20px;
  width: 100%;
}

.go-list{
  margin: 0 5px;
  flex: 1;
  padding: 2px 5px;
  font-size: 12px;
  border: 1px solid #ccc;
  text-align: center;
}

